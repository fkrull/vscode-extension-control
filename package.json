{
    "name": "extension-control",
    "displayName": "Extension Control",
    "description": "Manage VS Code extensions",
    "version": "0.0.1",
    "publisher": "fkrull",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extensionControl.syncLocalExtensions"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extensionControl.syncLocalExtensions",
                "title": "Extension Control: Sync Local Extensions"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "unittest": "npm run compile && node ./node_modules/mocha/bin/_mocha --recursive --ui=tdd out/test/unit"
    },
    "dependencies": {
        "fs-extra": "^4.0.2"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "tslint": "^5.7.0",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "ts-mockito": "^2.1.1",
        "typemoq": "^2.0.1",
        "tmp": "^0.0.33",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "@types/tmp": "^0.0.33",
        "@types/fs-extra": "^4.0.2"
    }
}
